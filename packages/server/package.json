{
  "name": "fast-ws-server",
  "version": "2.3.1",
  "description": "A simple Node.js server based on uWebSockets",
  "main": "js/index.js",
  "gypfile": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hans00/fastWS.git"
  },
  "bugs": {
    "url": "https://github.com/hans00/fastWS/issues"
  },
  "keywords": [
    "uWebSockets",
    "WebSocket",
    "web-server",
    "http-server",
    "https-server",
    "server",
    "fast-ws"
  ],
  "homepage": "https://github.com/hans00/fastWS#readme",
  "author": "Hans <admin@itsrv.tw>",
  "license": "Apache-2.0",
  "scripts": {
    "install": "node-gyp-build",
    "build": "node-gyp rebuild",
    "pre-build": "npx prebuildify -t 14.0.0 -t 16.0.0 -t 17.0.1 -t 18.0.0 --strip",
    "pre-build-arm-docker": "docker run --user $(id -u):$(id -g) --rm -v $PWD:/app ghcr.io/prebuild/linux-arm64:2 npm run pre-build-arm",
    "pre-build-arm": "npx prebuildify -t 14.0.0 -t 16.0.0 -t 17.0.1 -t 18.0.0 --strip --tag-armv",
    "clean": "rimraf build node_modules prebuilds"
  },
  "dependencies": {
    "accepts": "^1.3.8",
    "bindings": "^1.5.0",
    "bytes": "^3.1.2",
    "content-type": "^1.0.4",
    "iconv-lite": "^0.6.3",
    "mime-types": "^2.1.35",
    "multipart-formdata": "^1.1.0",
    "node-gyp-build": "^4.4.0",
    "prebuild-install": "^7.1.0",
    "qs": "^6.10.3",
    "range-parser": "^1.2.1",
    "replicator": "^1.0.5"
  },
  "devDependencies": {
    "node-gyp": "^8.2.0",
    "prebuildify": "^5.0.0",
    "rimraf": "^3.0.2"
  },
  "gitHead": "e939a8cb4e569fc3155c95c942470ab279e9d77d"
}
