{
  "name": "fast-ws",
  "version": "1.0.0",
  "description": "Simple Node.js server based on uWebSockets",
  "main": "src/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "git submodule update --init",
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild --all --verbose --include-regex \"uWS.*node$\"",
    "prebuild-upload": "prebuild --upload-all",
    "prebuild-ci": "prebuild-ci",
    "gypclean": "node-gyp clean",
    "gypconfigure": "node-gyp configure",
    "gypbuild": "node-gyp build",
    "gyprebuild": "node-gyp rebuild",
    "clean": "rimraf build node_modules prebuilds",
    "distclean": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hans00/fastWS.git"
  },
  "keywords": [
    "uWebSockets",
    "WebSocket"
  ],
  "author": "Hans <admin@itsrv.tw>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hans00/fastWS/issues"
  },
  "homepage": "https://github.com/hans00/fastWS#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "inet_xtoy": "^1.0.4",
    "prebuild-install": "^5.3.3"
  },
  "gypfile": true,
  "devDependencies": {
    "prebuild": "^9.1.1",
    "rimraf": "^3.0.0"
  }
}
